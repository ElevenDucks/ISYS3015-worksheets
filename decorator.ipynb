    "def decorator(f):\n",
    "    def new_function():\n",
    "        print(\"Extra Functionality\")\n",
    "        f()\n",
    "    return new_function\n",
    "\n",
    "@decorator\n",
    "def initial_function():\n",
    "    print(\"Initial Functionality\")\n",
    "\n",
    "initial_function()"
